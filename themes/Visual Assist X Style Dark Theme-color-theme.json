	"tokenColors": [
		{
			"settings": {
				"foreground": "#eeffffff",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6A9955"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other",
				"string constant.other.placeholder",
				"source.lua",
				"entity.name.variable",
                "meta.definition.variable.name",
                "support.variable",
                "variable.other.property",
                "variable.other.object.property"
			],
			"settings": {
				"foreground": "#BDB76B",
                "fontStyle": "italic"
			}
		},
        {
            "name": "Local Variable",
            "scope":[
                "entity.name.variable.local",
                "meta.block variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#BDB76B",
                "fontStyle": "bold"
            }
        },
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter.function",
				"entity.name.variable.parameter",
				"variable.other.readwrite"
			],
			"settings": {
				"foreground": "#BDB76B",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Class, Type",
			"scope": [
				// "storage.type",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
        {
            "name": "ts keyword",
            "scope":[
                "storage.type.function",
                "storage.type.type",
                "storage.type.interface",
                "support.type.primitive",
                "storage.type",
                "variable.language.this",
                // `await` in typescript
                "source meta.field.declaration keyword.control.flow",
                "source meta.arrow meta.function keyword.control.flow"
            ],
            "settings": {
                "foreground": "#569cd6",
            }
        },
		{
			"name": "Namespace",
			"scope":[
				"entity.name.namespace",
                "entity.name.type.namespace",
                "entity.name.type.module"
			],
			"settings": {
				"foreground": "#b8d68e"
			}
		},
		{
			"name": "Interface",
